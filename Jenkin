pipeline {
environment {
registry = "reach2johnny/demo"
registryCredential = 'dockerhub'
dockerImage = ''}
agent any
stages {
stage('Cloning Git') {
 steps {
 git 'https://github.com/johnsimon007/WebGoat.git'
 }
 }
stage('Building image') {
 steps{
 script {
 dockerImage = docker.build registry + ":$BUILD_NUMBER"
 }
 }
 }
